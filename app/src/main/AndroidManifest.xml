<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geekyquiz">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.geekyquiz.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.geekyquiz.EasyC"
            android:label="@string/title_activity_easy_c" />
        <activity
            android:name="com.geekyquiz.result"
            android:label="@string/title_activity_result" />
        <activity
            android:name="com.geekyquiz.Credits"
            android:label="@string/title_activity_credits"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
